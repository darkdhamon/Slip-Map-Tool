<Window x:Class="WPF_SlipMap.OpenSectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Open Sector" Height="300" Width="300">
   <Grid>
      <Grid.RowDefinitions>
         <RowDefinition />
         <RowDefinition Height="Auto" />
      </Grid.RowDefinitions>
      <ScrollViewer>
         <ListBox Name="Sectors" />
      </ScrollViewer>
      <Button Grid.Row="1" Click="Open_Click">Open Sector</Button>
   </Grid>
</Window>